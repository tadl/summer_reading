<h1>Listing participants</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Library card</th>
      <th>Email</th>
      <th>Zip code</th>
      <th>Home library</th>
      <th>School</th>
      <th>Grade</th>
      <th>Age</th>
      <th>Club</th>
      <th>Awards</th>
    </tr>
  </thead>

  <tbody>
    <% @participants.each do |participant| %>
      <tr>
        <td><%= participant.id %></td>
        <td><%= participant.first_name %></td>
        <td><%= participant.last_name %></td>
        <td><%= participant.library_card %></td>
        <td><%= participant.email %></td>
        <td><%= participant.zip_code %></td>
        <td><%= participant.home_library %></td>
        <td><%= participant.school %></td>
        <td><%= participant.grade %></td>
        <td><%= participant.age %></td>
        <td><%= participant.club %></td>
        <td>
          <%= participant.experiences %>
                 
          
        </td> 

      </tr>
    <% end %>
  </tbody>
</table>

<br>


